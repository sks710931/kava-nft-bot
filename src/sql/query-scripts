SELECT COUNT(*) AS TotalNFTs FROM ERC721NFTOwners
SELECT COUNT(*) AS TotalTransfers FROM ERC721NFTTransfers


SELECT COUNT(DISTINCT NftContractAddress) AS TotalNFTContracts from ERC721NFTOwners


SELECT COUNT(*) AS TotalMints FROM ERC721NFTTransfers WHERE SentFrom = '0x0000000000000000000000000000000000000000'